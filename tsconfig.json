{
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": true,
    "suppressImplicitAnyIndexErrors": true,
    "forceConsistentCasingInFileNames": true,
    "target": "es2017",
    "moduleResolution": "node",
    "module": "commonjs",
    "esModuleInterop": true,
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "dist"
  }
}
